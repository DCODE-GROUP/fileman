{
    "name": "dcodegroup/fileman",
    "description": "Laravel package for managing files.",
    "authors": [
        {
            "name": "Dcode",
            "email": "admin@dcode.com.au"
        }
    ],
    "require": {
        "php" : "^8.0",
        "laravel/framework": "^8.0|^9.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DcodeGroup\\Fileman\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DcodeGroup\\Fileman\\FileManServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
